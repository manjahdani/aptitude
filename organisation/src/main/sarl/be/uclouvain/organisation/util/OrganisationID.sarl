package be.uclouvain.organisation.util

import java.util.UUID

/** 
 * @author manjah
 * 
 */
class OrganisationID {
	val contextID : UUID
	val defaultSpaceID : UUID

	new(ctxt : UUID, sid : UUID) {
		this.contextID = ctxt
		this.defaultSpaceID = sid
	}

	def getContextID {
		return this.contextID
	}

	def defaultSpaceID {
		return this.defaultSpaceID
	}
}
