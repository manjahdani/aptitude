package be.uclouvain.organisation.platform.util

/** 
 * @author manjah
 * 
 */
class MissionParameters {
	
	var sensitivity : int
	var optimalSearchEnabled : boolean
	new (s : int, o : boolean) {
		this.sensitivity = s
		this.optimalSearchEnabled = o
	}
	
	def isOptimalSearchEnabled : boolean{
		return optimalSearchEnabled
	}

	def getSensitivity() {
		return this.sensitivity
	}
}
