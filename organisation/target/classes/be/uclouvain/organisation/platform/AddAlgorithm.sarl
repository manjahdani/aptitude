event AddAlgorithm {
	
	var MissionSpace : OpenEventSpace
	var info : AlgorithmInfo
	
	new (ospace : OpenEventSpace, d : AlgorithmInfo){
		this.MissionSpace = ospace
		this.info=d 
	}
}

event AddObserver {

	var MissionSpace   : OpenEventSpace
	var SignalProvider : AlgorithmInfo // @FIXME not general
	var SignalReceiver : AlgorithmInfo
	var flag : int
	new (ospace : OpenEventSpace, s : AlgorithmInfo, r : AlgorithmInfo) {
		this.MissionSpace = ospace
		this.SignalProvider = s
		this.SignalReceiver = r 
	}
}


