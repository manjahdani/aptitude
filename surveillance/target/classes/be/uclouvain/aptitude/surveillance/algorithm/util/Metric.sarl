package be.uclouvain.aptitude.surveillance.algorithm.util

/** 
 * @author manjah
 * 
 */
class Metric {
	
	var scoreName : String
	var gtFilePath:String
	var params : String []
	
	new (score : String, gtPatch : String,  p : String [])
	{
		this.gtFilePath = gtPatch
		this.scoreName = score
		this.params = p 
	}
	def getScoreName{
		return this.scoreName
	}
	def getGtFilePatch{
		return this.gtFilePath
	}
	def getParams{
		return this.params
	}
}
