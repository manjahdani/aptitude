package be.uclouvain.organisation.told.util

/** 
 * 
 * 
 * @TODO : Comments
 */
 
class AlgorithmInfo {

	val name : String
	val belief : String
	val task : String

	new (n : String, b : String, t : String) {
		this.name = n
		this.belief = b
		this.task = t
	}
	


	def getName {
		return this.name
	}

	def getBelief {
		return this.belief;
	}

	def getTask {
		return this.task;
	}
}
