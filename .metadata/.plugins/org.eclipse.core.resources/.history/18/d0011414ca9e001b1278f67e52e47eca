package be.uclouvain.aptitude.surveillance.algorithm

import io.sarl.core.OpenEventSpace
import io.sarl.lang.core.AgentContext

/** 
 * @author manjah
 * 
 */
event AlgorithmNeeded {
	val ContextID : AgentContext
	val SpaceID : OpenEventSpace
	val name : String
	val task : String
	val belief : String

	new (context : AgentContext, Space : OpenEventSpace, n : String, t : String, bel : String) {
		this.ContextID = context
		this.SpaceID = Space
		this.task = t;
		this.belief = bel;
		this.name = n

	}
}
