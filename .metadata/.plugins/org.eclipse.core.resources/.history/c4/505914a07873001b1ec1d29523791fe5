package be.uclouvain.aptitudeAgents

import io.sarl.core.Logging
import io.sarl.core.Initialize

/** 
 * @author manjah
 * 
 */
agent Algorithm {
	uses Logging
	
	var belief : int; 
	var goal : int;
	
	on Initialize {
		
		belief = occurrence.parameters.get(0) as String
		loggingName = "ALGORITHM" + this.ID
		
		info("Algorithm " + belief+ " was started.")
	} 


}
