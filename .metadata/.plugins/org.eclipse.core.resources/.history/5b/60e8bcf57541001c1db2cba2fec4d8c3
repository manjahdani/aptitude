package be.uclouvain.aptitude.surveillance.algorithm


import io.sarl.lang.core.AgentContext
import java.util.UUID

/** 
 * @TODO : Comment

 * @author $Author: manjahdani$
 * @version $0.0.2$
 * @date $16/04/2021$
 * @mavengroupid $be.uclouvain.aptitude$
 * @mavenartifactid $surveillance$
 * 
 */

event AlgorithmNeeded {
	
	var contextID : AgentContext
	var name : String
	var task : String
	var belief : String
	var sourceID: UUID
	new (ctxt : AgentContext, n : String, t : String, bel : String, sourceID : UUID) {
		this.contextID = ctxt
		this.task = t;
		this.belief = bel;
		this.name = n
		this.sourceID = sourceID

	}
}
