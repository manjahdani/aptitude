package be.uclouvain.organisation.told

/** 
 * Provide tools to access an (internal-external) database 
 * 
 * @author $Author: manjahdani$
 * @version $0.0.1$
 * @date $31/03/2021$
 * @mavengroupid $be.uclouvain.aptitude$
 * @mavenartifactid $organisation$
 */
 
capacity AccessDatabaseCapacity {
	
	def read(key:String) : Object    // enables the access to an entry in a database 
	def create(key : String, data : Object) // enables the creation an entry in a database 
	def update() // enables the update of an entry in a database 
	def delete(key:String) // enables the deletion of an entry in a database	 
}
